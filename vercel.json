{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "api/compare.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/compare", "dest": "api/compare.py" },
    { "src": "/api/process", "dest": "api/index.py" },
    { "src": "/(.*)", "dest": "api/index.py" }
  ]
}
